        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdfs:comment xml:lang="en">Determines a URL of a service in which the data and metadata of the dataset are stored. Accepts values in xsd:anyURI.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/ontouml-models/vocabulary"/>
        <rdfs:label xml:lang="en">storage URL</rdfs:label>
    </owl:DatatypeProperty>
    


