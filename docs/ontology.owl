        <rdfs:comment xml:lang="en">Identifies an ocmv:OntologyRepresentationStyle representation styles adopted in the artefact.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/ontouml-models/vocabulary"/>
        <rdfs:label xml:lang="en">ontology type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontouml-models/vocabulary#representationStyle -->


