        <rdfs:comment xml:lang="en">Identifies an ocmv:Context in which the artefact was developed.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/ontouml-models/vocabulary"/>
        <rdfs:label xml:lang="en">context</rdfs:label>
    </owl:ObjectProperty>
    


